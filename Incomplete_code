# ------------------------------------------------- Maps ------------------------------------------------------------ #

def map():

    #fmap_pic_label.img = fmap_img   # to keep the reference for the image.
    #fmap_pic_label.place(x=-10, y=-10)   # <--- place
    #frame_map.place(relx=0.15, rely=0.14, relwidth=0.7, relheight=0.7)
    ma.place(x=83, y=95)
    mt.place(x=245, y=65)

def map1():

    m1.place_forget()
    b4.place_forget()
    y1.place_forget()
    #f4_pic_label.pack_forget()
    c1 = tk.Button(frame, text='Close', bg='black', fg='white', font=('Bradley Hand ITC', 20, 'bold'), border=2,
                   command=frame4)
    map()
    c1.place(x=704, y=630)

