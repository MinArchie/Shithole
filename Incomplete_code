        mt.place_forget()

    # f10c_pic_label.img = f5_img
    # f10c_pic_label.pack()

    y7c.place(x=35, y=150)

    m1 = tk.Button(frame, text='Map', bg='black', fg='white', font=('Bradley Hand ITC', 20, 'bold'), border=2,
                   command=map1)
    m1.place(x=15, y=15)

    b4.place(x=710, y=630)

